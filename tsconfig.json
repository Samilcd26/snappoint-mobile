{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        ".//*"
      ],
      "~/*": [
        "./src/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@screens/*": [
        "./app/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@api/*": [
        "./api/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@constants/*": [
        "./constants/*"
      ]
    }
  }
}